{
	"front":
	{
		"/":
		{
			"*":
			{
				"controller":"index",
				"action":"index"
			}
		},
		"hi":
		{
			"GET|POST":
			{
				"controller":"Auth",
				"action":"login"
			}
		},
		"bye":
		{
			"GET":
			{
				"controller":"Auth",
				"action":"logout"
			}
		},
		"t/{$tag}":
		{
			"parameters":
			{
				"tag":"[a-z]+"
			},
			"GET":
			{
				"controller":"Photos",
				"action":"byTag"
			}
		},
		"t/{$tag}/{$file}":
		{
			"parameters":
			{
				"tag":"[a-z]+",
				"file":".+"
			},
			"GET":
			{
				"controller":"Photos",
				"action":"byTagAndPath"
			}
		},
		"newuser":
		{
			"GET":
			{
				"controller":"index",
				"action":"newUser"
			}
		}
	}
}